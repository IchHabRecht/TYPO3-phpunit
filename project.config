[receive]
	requireChangeId = true
[access "refs/heads/*"]
	label-Code-Review = -2..+2 group TYPO3CMS/Extensions/phpunit-Leaders
	label-Verified = -1..+2 group TYPO3CMS/Extensions/phpunit-Leaders
	submit = group TYPO3CMS/Extensions/phpunit-Leaders
	create = group TYPO3CMS/Extensions/phpunit-Leaders
	label-Code-Review = -2..+2 group TYPO3CMS/Extensions/phpunit-Members
	label-Verified = -1..+2 group TYPO3CMS/Extensions/phpunit-Members
	submit = group TYPO3CMS/Extensions/phpunit-Members
[access "refs/tags/*"]
	pushTag = group TYPO3CMS/Extensions/phpunit-Leaders
	create = group TYPO3CMS/Extensions/phpunit-Leaders
[project]
	state = active
[access "refs/for/refs/heads/*"]
	pushMerge = group TYPO3CMS/Extensions/phpunit-Leaders
	pushMerge = group TYPO3CMS/Extensions/phpunit-Members
	forgeCommitter = group TYPO3CMS/Extensions/phpunit-Leaders
[submit]
	action = cherry pick
	mergeContent = true
[access "refs/*"]
	owner = group TYPO3CMS/Extensions/phpunit-Leaders
